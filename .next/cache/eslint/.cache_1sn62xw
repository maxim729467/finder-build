Container\";\nimport Link from \"next/link\";\nimport Image from \"next/image\";\nimport { useRouter } from \"next/router\";\n\nimport logo from \"public/assets/images/mainPage/logo.png\";\nimport classes from \"../../Header.module.scss\";\n\nconst NavBar = () => {\n  const router = useRouter();\n  const navRef = useRef();\n  const [clientWindowHeight, setClientWindowHeight] = useState();\n\n  const handleScroll = () => {\n    if (!navRef.current) return;\n    const mobileWidth = window.innerWidth <= 768;\n    if (window.innerWidth <= 768) return;\n\n    if (window.scrollY > clientWindowHeight) {\n      // navRef.current.style.top = mobileWidth ? \"-100px\" : \"-500px\";\n      navRef.current.style.opacity = 0;\n      navRef.current.style.pointerEvents = \"none\";\n    } else {\n      // navRef.current.style.top = \"0px\";\n      navRef.current.style.opacity = 1;\n      navRef.current.style.pointerEvents = \"initial\";\n    }\n\n    setClientWindowHeight(window.scrollY);\n  };\n\n  useEffect(() => {\n    window.addEventListener(\"scroll\", handleScroll);\n    return () => window.removeEventListener(\"scroll\", handleScroll);\n  });\n\n  return (\n    <div className={classes.NavAnimated} ref={navRef}>\n      <Container>\n        <div className={classes.NavWrapper}>\n          <Link href=\"/\">\n            <Image\n              className={classes.Logo}\n              src={logo}\n              alt=\"герб Оазиса\"\n              width={240}\n              height={100}\n            />\n          </Link>\n          <nav className={classes.Nav}>\n            <ul className={classes.NavList}>\n              <li className={classes.NavLink}>\n                <Link href=\"/\">\n                  <a\n                    className={\n                      router.pathname === \"/\"\n                        ? classes.isActive\n                        : classes.isInactive\n                    }\n                  >\n                    Главная\n                  </a>\n             